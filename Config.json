      // 大于1的不重复数字
      "QLkey": 1,
      // 服务器昵称
      "QLName": "青龙面板",
      // 青龙访问地址
      "QLurl": "",
      // 青龙的应用ID
      "QL_CLIENTID": "",
      // 青龙的应用SECRET
      "QL_SECRET": "",
      // CK最大容量
      "QL_CAPACITY": 350,
      // WSCK最大容量
      "QL_WSCK": 40,
      // WXPUSHER的应用token，不推送留空
      "APP_TOKEN": ""
    }
