  "MaxTab": "10",
  // 标题
  "Title": "大灰狼京东面板",
  // 浏览器回收时间
  "Closetime": "5",
  // 公告
  "Announcement": "为提高账户的安全性，请关闭免密支付。",
  // 是否开启debug模式，默认关闭，开启设置为true
  "debug": false,
  // 自动过验证码的次数
  "AutoCaptchaCount": "5",
  // 无密码认证的http代理或socks代理
  "proxy": "",
  // XDD_URL地址
  "XDD_URL": "",
  // XDD_TOKEN
  "XDD_TOKEN": "",
  // 是否开启log计算服务，默认不开启
